# THSRC Ticket Bot - Environment Variables
# Copy this file to .env and fill in your values

# Web interface password (required for security)
APP_PASSWORD=your_secure_password

# Google Gemini API key for better captcha OCR accuracy (recommended)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key

# Server port (optional, default: 8080)
PORT=8080

# Debug mode (optional, default: false)
DEBUG=false
